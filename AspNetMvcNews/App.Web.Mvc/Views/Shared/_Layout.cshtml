<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="description" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewData["Title"] - App.Web.Mvc</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/App.Web.Mvc.styles.css" asp-append-version="true" />
    <link rel="icon" href="~/NewsTemplate/img/core-img/favicon.ico">

    <!-- Core Style CSS -->
    <link rel="stylesheet" href="~/NewsTemplate/css/core-style.css">
    <link rel="stylesheet" href="~/NewsTemplate/css/style.css">

    <!-- Responsive CSS -->
    <link href="~/NewsTemplate/css/responsive.css" rel="stylesheet">
</head>
<body>
    <header class="header-area">
        <partial name="_TopHeader" />
        <partial name="_MiddleHeader" />
        <partial name="_BottomHeader" />
    </header>
    <div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <partial name="_Footer" />



    <script src="~/newstemplate/js/jquery/jquery-2.2.4.min.js"></script>
    <script src="~/newstemplate/js/popper.min.js"></script>

    <script src="~/newstemplate/js/bootstrap.min.js"></script>

    <script src="~/newstemplate/js/plugins.js"></script>
    <script src="~/newstemplate/js/active.js"></script>

    @*<script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>*@
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
